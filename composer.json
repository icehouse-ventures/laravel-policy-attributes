{
    "name": "icehouse-ventures/laravel-policy-attributes",
    "description": "Laravel policy resource authorisation mapping using php attributes.",
    "keywords": ["laravel", "policy", "attributes", "icehouseventures"],
    "license": "MIT",
    "authors": [
        {
            "name": "Icehouse Ventures",
            "email": "tech@icehouseventures.co.nz"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/support": "^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0 || ^11.0",
        "illuminate/view": "^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0 || ^11.0",
        "illuminate/filesystem": "^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0 || ^11.0",
        "illuminate/collections": "^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0 || ^11.0"
    },
    "autoload": {
        "psr-4": {
            "IcehouseVentures\\LaravelPolicyAttributes\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "IcehouseVentures\\LaravelPolicyAttributes\\ServiceProvider"
            ],
            "aliases": {
                "PolicyAttributes": "IcehouseVentures\\LaravelPolicyAttributes\\Facades\\PolicyAttributes"
            }
        }
    }
}